{
  "manifest_version":3,
  "name":"MinTの情報取得アプリß",
  "version":"1.0",
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*" ],
    "js": ["jquery-3.6.0.min.js", "content-script.js"]
  }],
  "background": {
      "service_worker": "background.js"
  },
  "action": {
      "default_icon": "mint.png"
  },
  "permissions": [
      "tabs",
      "scripting",
      "background",
      "https://*/*",
      "http://*/*"
    ],
    "host_permissions": ["http://*/*", "https://*/*"]
  }